        <node id="1190" version="41b054b0-aed0-43f9-bca1-50629b7a5e4d" parentID="1156" level="4" writerID="0" creatorID="0" nodeType="1158" template="1053" sortOrder="14" createDate="2010-05-15T12:41:53" updateDate="2010-05-15T14:18:20" nodeName="Lakritz L" urlName="lakritz-l" writerName="Administrator" creatorName="Administrator" nodeTypeAlias="Häst" path="-1,1103,1152,1156,1190">
